{% extends '../BaseTransformer.jinja' %}

{% block transform %}
    for (uint16_t i = 0; i < {{ input_dim }}; i++) {
        current = source[i];
        source[i] -= old[i];
        old[i] = current;
    }

{% endblock %}

{% block protected %}

    float current;
    float old[{{ input_dim }}] = {0};

{% endblock %}