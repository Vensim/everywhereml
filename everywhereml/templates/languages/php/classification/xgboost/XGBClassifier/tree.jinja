{% if left[i] != right[i] %}
    if (x[{{ features[i] }}] <= {{ thresholds[i] }}) {
        {% with i = left[i] %}
            {% include './tree.jinja' %}
        {% endwith %}
    }
    else {
        {% with i = right[i] %}
            {% include './tree.jinja' %}
        {% endwith %}
    }
{% else %}
    votes[{{ k % num_classes }}] += {{ thresholds[i] }};
{% endif %}